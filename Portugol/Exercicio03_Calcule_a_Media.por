programa {
  funcao inicio() {
    
    real n1, n2, n3, media
    
    escreva("Digite o primeira nota: ")
    leia(n1)
    escreva("Digite o segunda nota: ")
    leia(n2)
    escreva("Digite o terceira nota: ")
    leia(n3)

    media = (n1 + n2 + n3) / 3

    escreva("A média entre as notas ", n1, ", ", n2, " e ", n3, " é igual a ", media, ".\n")

  }
}
